{
  "AllAutocomplete.updateOnlyOnSave": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
  "editor.folding": false,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,

  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 25,
  "editor.suggest.localityBonus": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "emmet.showExpandedAbbreviation": "never",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "fb.ms-remote-connections.odtypes": [
    {
      "type": "ondemand_shellserver",
      "flavor": "default",
      "folders": []
    },
    {
      "type": "ondemand_mobile",
      "flavor": "default",
      "folders": []
    }
  ],
  "files.exclude": {
    "**/.dist": true,
    "**/*.pyc": {
      "when": "$(basename).py"
    },
    "**/__pycache__": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/tmp": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/bower_components/**": true,
    "**/dist/**": true
  },
  "gitlens.codeLens.enabled": false,
  "hg.jumpToNextConflictOnSave": "Jump On Save And Open ISL When Done",
  "interactive-smartlog.autoRunMergeDrivers": true,
  "interactive-smartlog.fetch": "jf get and checkout",
  "interactive-smartlog.pull": "arc pull",
  "javascript.validate.enable": false,
  "search.exclude": {
    "**/.dist": true,
    "**/.git": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true
  },
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.profiles.osx": {
    "bash (login)": {
      "path": "bash",
      "args": ["-l"]
    }
  },
  "workbench.editor.untitled.hint": "hidden",
  "workbench.startupEditor": "newUntitledFile",
  "editor.suggestSelection": "first",
  "terminal.integrated.defaultProfile.osx": "bash",
  "editor.guides.indentation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.userWords": ["Pressable"],
  "editor.minimap.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {},
  "editor.wordWrapColumn": 160,
  "files.autoSaveDelay": 5000,
  "react-native-tools.showUserTips": false,
  "phpCodeSniffer.standard": "Custom",
  "phpCodeSniffer.standardCustom": "PSR2",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "window.zoomLevel": 1,
  "arduino.useArduinoCli": true,
  "editor.largeFileOptimizations": false,
  "phpCodeSniffer.autoExecutable": true,
  "phpCodeSniffer.exec.osx": "/Users/hramos/.composer/vendor/bin/phpcs",
  "editor.formatOnSave": true
}
